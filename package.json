        "@vue/cli-service": "^3.7.0",
        "node-sass": "4.14.1",
        "sass-loader": "^7.1.0",
        "terser": "^3.17.0",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@fuzzco/full-canvas": "1.1.0",
        "lodash": "4.17.20",
